{
    "name": "moritz-sauer-13/silverstripe-agegate",
    "description": "Adds age-gate for verifying visitor's eligibility to view restricted content",
    "keywords": ["age", "agegate", "gate", "silverstripe"],
    "type": "silverstripe-vendormodule",
    "homepage": "https://github.com/RevStrat/silverstripe-agegate",
    "license": "BSD-3-Clause",
    "authors": [{
        "name": "Rob Rankin",
        "email": "rob@revolutionstrategy.com"
    }],
    "require": {
        "silverstripe/framework": "^5",
        "geoip2/geoip2": "^3",
        "jaybizzle/crawler-detect" :"1.*",
        "silverstripe/siteconfig": "^5.4"
    },
    "extra": {
        "installer-name": "agegate",
        "expose": [
            "css",
            "javascript"
        ]
    },
    "autoload": {
        "psr-4": {
            "revstrat\\silverstripe-agegate\\": "src"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "allow-plugins": {
            "composer/installers": true,
            "silverstripe/vendor-plugin": true
        }
    }
}
